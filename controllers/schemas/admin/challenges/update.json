{
  "title": "AdminChallengesUpdate",
  "description": "Challenge update request",
  "type": "object",
  "properties": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "categories": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "points": {
      "type": "number"
    },
    "difficulty": {
      "type": "string",
      "enum": ["easy", "medium", "hard"]
    },
    "flag": {
      "type": "string"
    },
    "isLocked": {
      "type": "boolean"
    },
    "dependsOn": {
      "type": "number"
    }
  },
  "additionalProperties": false,
  "required": ["title", "categories", "points", "difficulty", "isLocked"]
}
